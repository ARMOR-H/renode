$bin?=@example/P3_unit_tests.elf

mach create
machine LoadPlatformDescription @example/p3.repl
machine StartGdbServer 3333

logLevel -1 sysbus.i2c.photodetector

emulation CreateServerSocketTerminal 12345 "term" 
connector Connect sysbus.usart term

macro reset
"""
    sysbus LoadELF $bin
"""
runMacro $reset

emulation CreateEnvironment "env"
machine SetEnvironment env
start