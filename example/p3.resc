$bin?=@example/p3.elf

mach create
machine LoadPlatformDescription @example/p3.repl
machine StartGdbServer 3333

logLevel -1 sysbus.i2c.photodetector

emulation CreateServerSocketTerminal 12345 "term" false


macro reset
"""
    sysbus LoadELF $bin
"""
runMacro $reset
connector Connect sysbus.ble.proxy term


# emulation CreateEnvironment "env"
# machine SetEnvironment env
# cpu LogFunctionNames true "HAL_ ble_"

logLevel -1 sysbus.ble
logLevel -1 sysbus.ble.proxy
start